{
  "canisters": {
    
    "user": {
      "type": "motoko",
      "main": "backend/user/main.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "symptom": {
      "type": "motoko",
      "main": "backend/symptom/main.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "history": {
      "type": "motoko",
      "main": "backend/history/main.mo",
      "declarations": {
        "node_compatibility": true
      }
    },
    "internet_identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "frontend": {
      "type": "assets",
      "source": ["dist/"],
      "dependencies": ["internet_identity"]
